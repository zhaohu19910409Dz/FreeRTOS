<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.john.ctronnel">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recycleView.GridRecyclerViewActivity"></activity>
        <activity android:name=".recycleView.HorRecyclerViewActivity" />
        <activity android:name=".recycleView.LinearRecycleViewActivity" />
        <activity android:name=".recycleView.recycleViewActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".listView.ListActivity" />
        <activity android:name=".gridview.GridViewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>